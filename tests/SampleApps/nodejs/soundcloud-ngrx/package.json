{
  "name": "soundcloud-ngrx",
  "version": "1.0.0",
  "description": "SoundCloud API client with Angular2 and NgRx",
  "homepage": "https://github.com/r-park/soundcloud-ngrx",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/soundcloud-ngrx.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "run-p clean:build clean:dist",
    "clean:build": "del-cli build",
    "clean:coverage": "del-cli coverage",
    "clean:dist": "del-cli dist",
    "heroku-postbuild": "npm run build",
    "lint": "run-s lint:js lint:ts",
    "lint:js": "eslint -c .eslintrc.yml *.js server",
    "lint:ts": "tslint 'src/**/*.ts'",
    "prebuild": "npm run clean",
    "pretest": "run-s clean:coverage lint",
    "pretest:server": "npm run build",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm run server:dev",
    "test": "run-s test:client test:server",
    "test:client": "cross-env NODE_ENV=test karma start --single-run",
    "test:server": "ava server/spec.js --verbose",
    "test:watch": "cross-env NODE_ENV=test karma start --auto-watch",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "compression": "~1.6.2",
    "express": "~4.15.3",
    "helmet": "~3.6.1",
    "serve-favicon": "~2.4.3",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "@angular/common": "4.2.4",
    "@angular/compiler": "4.2.4",
    "@angular/compiler-cli": "4.2.4",
    "@angular/core": "11.0.5",
    "@angular/forms": "4.2.4",
    "@angular/http": "4.2.4",
    "@angular/platform-browser": "4.2.4",
    "@angular/platform-browser-dynamic": "4.2.4",
    "@angular/platform-server": "4.2.4",
    "@angular/router": "4.2.4",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.3",
    "@ngrx/store": "2.2.2",
    "@types/jasmine": "2.5.45",
    "@types/node": "~8.0.5",
    "angular2-template-loader": "~0.6.2",
    "autoprefixer": "~7.1.1",
    "ava": "~0.20.0",
    "awesome-typescript-loader": "3.2.1",
    "conventional-changelog-cli": "~1.3.1",
    "core-js": "~2.4.1",
    "cross-env": "~5.0.1",
    "css-loader": "~0.28.4",
    "del-cli": "~1.1.0",
    "eslint": "~4.1.1",
    "html-webpack-plugin": "~2.29.0",
    "immutable": "~3.8.1",
    "istanbul-instrumenter-loader": "~2.0.0",
    "jasmine-core": "~2.6.4",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.1.0",
    "karma-remap-coverage": "~0.1.4",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~2.0.3",
    "minx": "r-park/minx.git",
    "morgan": "~1.8.2",
    "ngc-webpack": "~3.0.0",
    "node-sass": "~7.0.0",
    "nodemon": "~1.11.0",
    "npm-run-all": "~4.0.2",
    "postcss-loader": "~2.0.6",
    "raw-loader": "~0.5.1",
    "rxjs": "5.4.1",
    "sass-loader": "7.1.0",
    "style-loader": "~0.18.2",
    "supertest": "~3.0.0",
    "tslib": "~1.7.1",
    "tslint": "~5.4.3",
    "typescript": "~2.3.4",
    "webpack": "3.0.0",
    "webpack-dev-server": "2.5.0",
    "webpack-md5-hash": "~0.0.5",
    "zone.js": "~0.8.12"
  }
}
