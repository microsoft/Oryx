{
  "name": "soundcloud-ngrx",
  "version": "1.0.0",
  "description": "SoundCloud API client with Angular2 and NgRx",
  "homepage": "https://github.com/r-park/soundcloud-ngrx",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-park/soundcloud-ngrx.git"
  },
  "author": {
    "name": "Richard Park",
    "email": "objectiv@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=8.1"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "run-p clean:build clean:dist",
    "clean:build": "del-cli build",
    "clean:coverage": "del-cli coverage",
    "clean:dist": "del-cli dist",
    "heroku-postbuild": "npm run build",
    "lint": "run-s lint:js lint:ts",
    "lint:js": "eslint -c .eslintrc.yml *.js server",
    "lint:ts": "tslint 'src/**/*.ts'",
    "prebuild": "npm run clean",
    "pretest": "run-s clean:coverage lint",
    "pretest:server": "npm run build",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm run server:dev",
    "test": "run-s test:client test:server",
    "test:client": "cross-env NODE_ENV=test karma start --single-run",
    "test:server": "ava server/spec.js --verbose",
    "test:watch": "cross-env NODE_ENV=test karma start --auto-watch",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "4.18.2",
    "helmet": "^3.23.3",
    "serve-favicon": "~2.4.3",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "@angular/common": "16.0.5",
    "@angular/compiler": "16.0.5",
    "@angular/compiler-cli": "16.0.5",
    "@angular/core": "16.0.5",
    "@angular/forms": "16.0.5",
    "@angular/platform-browser": "16.0.5",
    "@angular/platform-browser-dynamic": "16.0.5",
    "@angular/platform-server": "16.0.5",
    "@angular/router": "16.0.5",
    "@ngrx/effects": "16.0.1",
    "@ngrx/store": "16.0.1",
    "@ngtools/webpack": "16.0.5",
    "@types/jasmine": "2.5.45",
    "@types/node": "~8.0.5",
    "angular2-template-loader": "~0.6.2",
    "autoprefixer": "^10.4.14",
    "ava": "^5.3.0",
    "awesome-typescript-loader": "3.2.1",
    "conventional-changelog-cli": "^3.0.0",
    "core-js": "~2.4.1",
    "cross-env": "~5.0.1",
    "css-loader": "^6.8.1",
    "del-cli": "^5.0.0",
    "eslint": "~4.1.1",
    "html-webpack-plugin": "5.5.3",
    "immutable": "~3.8.1",
    "jasmine-core": "~2.6.4",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-remap-coverage": "~0.1.4",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "5.0.0",
    "minx": "0.4.14",
    "morgan": "^1.10.0",
    "node-sass": "9.0.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "~4.0.2",
    "postcss-loader": "^7.3.3",
    "raw-loader": "~0.5.1",
    "rxjs": "6.5.3",
    "sass-loader": "13.3.2",
    "style-loader": "^3.3.3",
    "supertest": "~3.0.0",
    "tslib": "~1.7.1",
    "tslint": "~5.4.3",
    "typescript": "4.9.3",
    "webpack": "^5.86.0",
    "webpack-dev-server": "4.15.1",
    "webpack-md5-hash": "~0.0.5",
    "zone.js": "0.13.0"
  }
}
